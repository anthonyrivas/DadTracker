<div class="jumbotron jumbotron-fluid dad-bg mb-0">
    <div class="container-fluid">
        <h1>Welcome to the Dad Tracker</h1>
        <h3>The open source Dad Journal</h3>
    </div>
</div>
<div class="container-fluid bg-light pt-2">
    <h1>Life Gallery</h1>
    <hr/>
    <div class="row">
        {{#each images}}
        <div class="col-md-3">
            <div class="card bg-dark text-white mb-3">
                <img class="card-img" src="{{imageUrl}}" alt="{{value}}">
                <div class="card-img-overlay p-0 ">
                    <h5 class="bg-semidark absolute-bottom p-2 image-overlay-hide">{{value}}</h5>
                </div>
            </div>
        </div>
        {{/each}}
    </div>
</div>